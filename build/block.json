{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "gutenberg-blocks/button",
  "version": "0.1.0",
  "title": "GT Button",
  "category": "common",
  "icon": "smiley",
  "description": "Example static block scaffolded with Create Block tool.",
  "attributes": {
    "alignment": {
      "type": "string"
    },
    "bg_color": {
      "type": "string",
      "default": "#000000"
    },
    "text_color": {
      "type": "string",
      "default": "#ffffff"
    },
    "text": {
      "type": "string",
      "default": "Clicked Me!"
    },
    "target": {
      "type": "string"
    },
    "sourcepage": {
      "type": "string"
    },
    "pageID": {
      "type": "string"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "gutenberg-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}